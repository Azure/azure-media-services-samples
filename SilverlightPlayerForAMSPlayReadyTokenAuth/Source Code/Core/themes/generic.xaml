                                                <Path Stretch="Fill" Fill="#80000000" Data="F1 M 3.0228,2.04675L 3.0228,3.04614L 3.0228,3.04675L 1.0228,3.04675L 1.0228,2.04675L 2.0228,2.04675L 2.0228,0.0461426L 3.0228,0.0461426L 3.0228,2.04675 Z " Height="3.001" HorizontalAlignment="Left" Margin="2.808,1.859,0,0" VerticalAlignment="Top" Width="2"/>
                                                <Path Stretch="Fill" Fill="#80C3C3C3" Data="F1 M 0.0315755,5.03284L 0.0315755,4.03357L 0.0315755,4.03284L 2.03158,4.03284L 2.03158,5.03284L 1.03158,5.03284L 1.03158,7.03357L 0.0315755,7.03357L 0.0315755,5.03284 Z " Height="3.001" HorizontalAlignment="Left" Margin="1.817,5.846,0,0" VerticalAlignment="Top" Width="2"/>
                                                <Path Stretch="Fill" Fill="#80000000" Data="F1 M 3.01434,6.02209L 3.01434,7.02136L 3.01434,7.02209L 1.01434,7.02209L 1.01434,6.02209L 2.01434,6.02209L 2.01434,4.02136L 3.01434,4.02136L 3.01434,6.02209 Z " Height="3.001" HorizontalAlignment="Left" Margin="2.799,5.834,0,0" VerticalAlignment="Top" Width="2"/>
                                                <Path Stretch="Fill" Fill="#80C3C3C3" Data="F1 M 0.0201823,8.99841L 0.0201823,7.99902L 0.0201823,7.99841L 2.02018,7.99841L 2.02018,8.99841L 1.02018,8.99841L 1.02018,10.999L 0.0201823,10.999L 0.0201823,8.99841 Z " HorizontalAlignment="Left" Margin="1.805,9.93,0,7.07" Width="2"/>
                                                <Path Stretch="Fill" Fill="#80000000" Data="F1 M 3.00295,9.98816L 3.00295,10.9875L 3.00295,10.9882L 1.00295,10.9882L 1.00295,9.98816L 2.00295,9.98816L 2.00295,7.98755L 3.00295,7.98755L 3.00295,9.98816 Z " HorizontalAlignment="Left" Margin="2.788,9.919,0,7.081" Width="2"/>
                                                <Path Stretch="Fill" Fill="#80C3C3C3" Data="F1 M 0.0117188,12.974L 0.0117188,11.9746L 0.0117188,11.974L 2.01172,11.974L 2.01172,12.974L 1.01172,12.974L 1.01172,14.9746L 0.0117188,14.9746L 0.0117188,12.974 Z " Height="3.001" HorizontalAlignment="Left" Margin="1.797,0,0,2.784" VerticalAlignment="Bottom" Width="2"/>
                                                <Path Stretch="Fill" Fill="#80000000" Data="F1 M 2.99448,13.9635L 2.99448,14.9628L 2.99448,14.9635L 0.994484,14.9635L 0.994484,13.9635L 1.99448,13.9635L 1.99448,11.9628L 2.99448,11.9628L 2.99448,13.9635 Z " Height="3.001" HorizontalAlignment="Left" Margin="2.779,0,0,2.795" VerticalAlignment="Bottom" Width="2"/>
                                                <Path Stretch="Fill" Fill="#80C3C3C3" Data="F1 M 4.01449,1.04333L 4.01449,0.0439453L 4.01449,0.043335L 6.01449,0.043335L 6.01449,1.04333L 5.01449,1.04333L 5.01449,3.04395L 4.01449,3.04395L 4.01449,1.04333 Z " Height="3.001" HorizontalAlignment="Right" Margin="0,1.856,3.201,0" VerticalAlignment="Top" Width="2"/>
