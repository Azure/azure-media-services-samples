                                                <Path Stretch="Fill" Fill="#80C3C3C3" Data="F1 M 4.00602,5.01892L 4.00602,4.01953L 4.00602,4.01892L 6.00602,4.01892L 6.00602,5.01892L 5.00602,5.01892L 5.00602,7.01953L 4.00602,7.01953L 4.00602,5.01892 Z " Height="3.001" HorizontalAlignment="Right" Margin="0,5.832,3.209,0" VerticalAlignment="Top" Width="2"/>
                                                <Path Stretch="Fill" Fill="#80000000" Data="F1 M 6.98879,6.00769L 6.98879,7.00708L 6.98879,7.00769L 4.98879,7.00769L 4.98879,6.00769L 5.98879,6.00769L 5.98879,4.00708L 6.98879,4.00708L 6.98879,6.00769 Z " Height="3.001" HorizontalAlignment="Right" Margin="0,5.82,2.226,0" VerticalAlignment="Top" Width="2"/>
                                                <Path Stretch="Fill" Fill="#80C3C3C3" Data="F1 M 3.99463,8.98438L 3.99463,7.98499L 3.99463,7.98438L 5.99463,7.98438L 5.99463,8.98438L 4.99463,8.98438L 4.99463,10.985L 3.99463,10.985L 3.99463,8.98438 Z " HorizontalAlignment="Right" Margin="0,10.301,2.966,6.699" Width="2" d:LayoutOverrides="VerticalAlignment"/>
                                                <Path Stretch="Fill" Fill="#80000000" Data="F1 M 6.97739,9.97388L 6.97739,10.9733L 6.97739,10.9739L 4.97739,10.9739L 4.97739,9.97388L 5.97739,9.97388L 5.97739,7.97327L 6.97739,7.97327L 6.97739,9.97388 Z " HorizontalAlignment="Right" Margin="0,10.289,1.985,6.711" Width="2" d:LayoutOverrides="VerticalAlignment"/>
                                                <Path Stretch="Fill" Fill="#80C3C3C3" Data="F1 M 3.98633,12.96L 3.98633,11.9606L 3.98633,11.96L 5.98633,11.96L 5.98633,12.96L 4.98633,12.96L 4.98633,14.9606L 3.98633,14.9606L 3.98633,12.96 Z " Height="3.001" HorizontalAlignment="Right" Margin="0,0,3.229,2.798" VerticalAlignment="Bottom" Width="2"/>
                                                <Path Stretch="Fill" Fill="#80000000" Data="F1 M 6.96893,13.9491L 6.96893,14.9485L 6.96893,14.9491L 4.96893,14.9491L 4.96893,13.9491L 5.96893,13.9491L 5.96893,11.9485L 6.96893,11.9485L 6.96893,13.9491 Z " Height="3.001" HorizontalAlignment="Right" Margin="0,0,2.246,2.81" VerticalAlignment="Bottom" Width="2"/>
                                            </Grid>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                            </Style>
                        </Grid.Resources>
                        <Grid x:Name="HorizontalTemplate" Background="{TemplateBinding Background}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Rectangle Height="Auto" Margin="0,0,0,0" Grid.Column="0" Grid.ColumnSpan="3" RadiusX="1" RadiusY="1" Fill="{StaticResource metal_grey}" Stroke="#FFFFFFFF"/>
                            <RepeatButton IsTabStop="False" Template="{StaticResource RepeatButtonTemplate}" x:Name="HorizontalTrackLargeChangeDecreaseRepeatButton" Grid.Column="0" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}"/>
                            <Rectangle Cursor="Hand" x:Name="LeftTrack" Grid.Column="0" Fill="{StaticResource green_glass2}"/>
                            <Thumb IsTabStop="True" x:Name="HorizontalThumb" Width="11" Grid.Column="1" Style="{StaticResource volume_thumb}" Height="20"/>
                            <RepeatButton IsTabStop="False" Template="{StaticResource RepeatButtonTemplate}" x:Name="HorizontalTrackLargeChangeIncreaseRepeatButton" Grid.Column="2" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}"/>
                            <Rectangle Cursor="Hand" x:Name="RightTrack" Grid.Column="2" Fill="#00FFFFFF"/>
                        </Grid>
                        <Grid x:Name="VerticalTemplate" Background="{TemplateBinding Background}" Visibility="Collapsed">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*"/>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition Height="Auto"/>
                            </Grid.RowDefinitions>
                            <Rectangle Margin="0,5,0,5" Width="3" Grid.Row="0" Grid.RowSpan="3" RadiusX="1" RadiusY="1" Fill="#FFE6EFF7" Stroke="#FFA3AEB9" StrokeThickness="{TemplateBinding BorderThickness}"/>
                            <RepeatButton IsTabStop="False" Template="{StaticResource RepeatButtonTemplate}" x:Name="VerticalTrackLargeChangeDecreaseRepeatButton" Grid.Row="2"/>
                            <Rectangle Cursor="Hand" x:Name="DownTrack" Grid.Row="2" Fill="#00FFFFFF"/>
                            <Thumb IsTabStop="True" Height="11" x:Name="VerticalThumb" Width="18" Grid.Row="1"/>
                            <RepeatButton IsTabStop="False" Template="{StaticResource RepeatButtonTemplate}" x:Name="VerticalTrackLargeChangeIncreaseRepeatButton" Grid.Row="0"/>
                            <Rectangle Cursor="Hand" x:Name="UpTrack" Grid.Row="0" Fill="#00FFFFFF"/>
                        </Grid>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="SmallChange" Value="10"/>
        <Setter Property="LargeChange" Value="100"/>
    </Style>

    <Style TargetType="player:MaxBitrateLimiterControl">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="player:MaxBitrateLimiterControl">
                    <Border Height="Auto" Width="400" HorizontalAlignment="Stretch" Margin="0" BorderThickness="2" CornerRadius="5" Opacity="{TemplateBinding Opacity}" x:Name="BitrateGraphBorder">
                        <Border.BorderBrush>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF767676"/>
                                <GradientStop Color="#002F2F2F" Offset="1"/>
                                <GradientStop Color="#FF494949" Offset="0.628"/>
                            </LinearGradientBrush>
                        </Border.BorderBrush>
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#4C767676"/>
                                <GradientStop Color="#7F2F2F2F" Offset="1"/>
                            </LinearGradientBrush>
                        </Border.Background>
                        <Grid Height="Auto" HorizontalAlignment="Stretch" Margin="5,0,5,0">
                            <Grid.Resources>
                                <Style x:Key="BITRATE_DISPLAY" TargetType="Slider">
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="Slider">
                                                <Grid x:Name="Root">
                                                    <Grid.Resources>
                                                        <ControlTemplate x:Key="RepeatButtonTemplate">
                                                            <Grid x:Name="Root" Background="Transparent" Opacity="0"/>
                                                        </ControlTemplate>
                                                    </Grid.Resources>
                                                    <Grid x:Name="HorizontalTemplate" Background="{TemplateBinding Background}">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition Width="Auto"/>
                                                            <ColumnDefinition Width="Auto"/>
                                                            <ColumnDefinition Width="*"/>
                                                        </Grid.ColumnDefinitions>
                                                        <Rectangle Height="Auto" Margin="0,0,0,0" Grid.Column="0" Grid.ColumnSpan="3" RadiusX="1" RadiusY="1" Fill="{StaticResource metal_grey}" Stroke="#FFFFFFFF"/>
                                                        <RepeatButton IsTabStop="False" Template="{StaticResource RepeatButtonTemplate}" x:Name="HorizontalTrackLargeChangeDecreaseRepeatButton" Grid.Column="0" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}"/>
                                                        <Rectangle Cursor="Hand" x:Name="LeftTrack" Grid.Column="0" Fill="{StaticResource green_glass2}"/>
                                                        <Thumb IsTabStop="True" x:Name="HorizontalThumb" Width="11" Grid.Column="1" Style="{StaticResource ThumbStyle2}" Height="20"/>
                                                        <RepeatButton IsTabStop="False" Template="{StaticResource RepeatButtonTemplate}" x:Name="HorizontalTrackLargeChangeIncreaseRepeatButton" Grid.Column="2" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}"/>
                                                        <Rectangle Cursor="Hand" x:Name="RightTrack" Grid.Column="2" Fill="#00FFFFFF"/>
                                                    </Grid>
                                                    <Grid x:Name="VerticalTemplate" Background="{TemplateBinding Background}" Visibility="Collapsed">
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="*"/>
                                                            <RowDefinition Height="Auto"/>
                                                            <RowDefinition Height="Auto"/>
                                                        </Grid.RowDefinitions>
                                                        <Rectangle Margin="0,5,0,5" Width="3" Grid.Row="0" Grid.RowSpan="3" RadiusX="1" RadiusY="1" Fill="#FFE6EFF7" Stroke="#FFA3AEB9" StrokeThickness="{TemplateBinding BorderThickness}"/>
                                                        <RepeatButton IsTabStop="False" Template="{StaticResource RepeatButtonTemplate}" x:Name="VerticalTrackLargeChangeDecreaseRepeatButton" Grid.Row="2"/>
                                                        <Rectangle Cursor="Hand" x:Name="DownTrack" Grid.Row="2" Fill="#00FFFFFF"/>
                                                        <Thumb IsTabStop="True" Height="11" x:Name="VerticalThumb" Width="18" Grid.Row="1"/>
                                                        <RepeatButton IsTabStop="False" Template="{StaticResource RepeatButtonTemplate}" x:Name="VerticalTrackLargeChangeIncreaseRepeatButton" Grid.Row="0"/>
                                                        <Rectangle Cursor="Hand" x:Name="UpTrack" Grid.Row="0" Fill="#00FFFFFF"/>
                                                    </Grid>
                                                </Grid>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </Grid.Resources>
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0" Height="Auto" x:Name="NowDowloadingBitrateGrid" Width="Auto" Margin="0,0,0,0" VerticalAlignment="Stretch">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Margin="0,0,0,0" x:Name="NowDownloadingBitrateHeader" VerticalAlignment="Stretch" Foreground="{StaticResource metal_grey}" Text="Now Downloading Bitrate" TextWrapping="NoWrap" HorizontalAlignment="Left" TextAlignment="Center" Grid.ColumnSpan="2"/>
                                <Slider IsHitTestVisible="False" Height="Auto" HorizontalAlignment="Stretch" x:Name="NowDownloadingSlider" VerticalAlignment="Top" Width="Auto" Grid.Row="1" IsDirectionReversed="False" Orientation="Horizontal" LargeChange="100" Maximum="5000" SmallChange="10" Style="{StaticResource BITRATE_DISPLAY}" Grid.ColumnSpan="2"/>
                                <StackPanel Height="Auto" Margin="0,0,0,0" x:Name="NowDownloadingBitrateLabelStackPanel" Width="Auto" Grid.RowSpan="1" Orientation="Horizontal" Grid.Column="1" d:LayoutOverrides="Width, Height" HorizontalAlignment="Right">
                                    <TextBlock Height="Auto" x:Name="NowDownloadingBitrateLabel" Width="Auto" Foreground="{StaticResource metal_grey}" Text="0" TextWrapping="Wrap" HorizontalAlignment="Stretch"/>
                                    <TextBlock Height="Auto" x:Name="NowDownloadingBitrateLabelUnits" Width="Auto" Foreground="{StaticResource metal_grey}" Text="kbps" TextWrapping="Wrap" HorizontalAlignment="Stretch" Margin="2,0,0,0"/>
                                </StackPanel>
                            </Grid>
                            <Grid Grid.Row="1" Height="Auto" x:Name="MaxBitrateLimiterGrid" VerticalAlignment="Stretch" Grid.Column="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
